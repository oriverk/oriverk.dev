{
  "name": "oriverk.dev",
  "version": "1.2.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/{*.ts,*.tsx}'",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:md": "run-s fix:markdownlint fix:textlint",
    "fix:eslint": "eslint --fix 'src/**/{*.ts,*.tsx}'",
    "fix:prettier": "prettier --write 'src/**/{*.ts,*.tsx}'",
    "fix:markdownlint": "markdownlint -f README.md",
    "fix:textlint": "textlint --fix README.md",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/oriverk.dev.git"
  },
  "keywords": [
    "vite",
    "preact",
    "typescript",
    "goober"
  ],
  "author": "oriverk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oriverk/oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk/oriverk.dev#readme",
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.9",
    "date-fns": "^2.28.0",
    "github-calendar": "^2.2.9",
    "goober": "^2.1.9",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.3.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.5.0",
    "@graphql-codegen/typescript-operations": "2.4.1",
    "@octokit/graphql-schema": "^10.73.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.3.2",
    "csstype": "^3.0.11",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.4",
    "markdownlint-cli": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "textlint": "^12.1.1",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8"
  }
}
