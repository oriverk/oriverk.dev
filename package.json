{
  "name": "oriverk.dev",
  "version": "1.2.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint 'src/**/{*.ts,*.tsx}'",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix 'src/**/{*.ts,*.tsx}'",
    "fix:prettier": "prettier --write 'src/**/{*.ts,*.tsx}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oriverk/oriverk.dev.git"
  },
  "keywords": [
    "vite",
    "react",
    "typescript",
    "goober"
  ],
  "author": "oriverk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oriverk/oriverk.dev/issues"
  },
  "homepage": "https://github.com/oriverk/oriverk.dev#readme",
  "dependencies": {
    "goober": "^2.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.10.0",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "csstype": "^3.1.2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  }
}
